<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <title>Merkata</title>
  <link rel="stylesheet" href="/stylesheets/style.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js" integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU=" crossorigin="anonymous"></script>
  <script src="/scripts/fade.js" type="text/javascript"></script>
  <script src="/scripts/home-buttons.js" type="text/javascript"></script>
</head>


<body>
  <!--[if lt IE 9]>
  <script>
    document.createElement('video');
  </script>
  <![endif]-->
  <video playsinline autoplay muted loop id="bgvid">
    <source src="/video/bgvid.mp4" type="video/mp4">
  </video>
  {{>nav}}
  <img id='open-nav' src='/images/nav.png' style='width: 40px; padding: 10px;'>
  <img id='mapicon' src='/images/mapicon.png'>

  <div id='overlay'></div>
  <div id='map-pop'>
    <div id='closemap'>&#10005;</div>
    <div id='dot'>
      <div id='dot-info'>
        Explore Guatemala
        <img src='/images/bgposter.png' style='width: 190px'>
      </div>
    </div>

  </div>
  <div id='welcome'>
    {{#if auth}}
      {{>home-auth}}
    {{else}}
      {{>home-noauth}}
    {{/if}}
  </div>
</body>

</html>